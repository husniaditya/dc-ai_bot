{
	"info": {
		"_postman_id": "dc-ai-bot-api-collection",
		"name": "Discord AI Bot API",
		"description": "Complete API collection for Discord AI Bot with comprehensive moderation, analytics, and media integration endpoints.\n\n**Base URL (Production):** https://chocomaid.xyz/api\n**Base URL (Development):** http://localhost:3000/api\n\n**Authentication:** JWT Bearer Token (use OAuth2 flow or legacy login)\n\n**Documentation:** https://github.com/husniaditya/dc-ai_bot",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
		"_exporter_id": "discord-ai-bot"
	},
	"auth": {
		"type": "bearer",
		"bearer": [
			{
				"key": "token",
				"value": "{{authToken}}",
				"type": "string"
			}
		]
	},
	"variable": [
		{
			"key": "baseUrl",
			"value": "https://chocomaid.xyz/api",
			"type": "string"
		},
		{
			"key": "authToken",
			"value": "",
			"type": "string"
		},
		{
			"key": "guildId",
			"value": "",
			"type": "string"
		}
	],
	"item": [
		{
			"name": "Authentication",
			"item": [
				{
					"name": "Get OAuth URL",
					"request": {
						"auth": {
							"type": "noauth"
						},
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{baseUrl}}/auth/oauth/discord/url?preferApp=false&isMobile=false",
							"host": ["{{baseUrl}}"],
							"path": ["auth", "oauth", "discord", "url"],
							"query": [
								{
									"key": "preferApp",
									"value": "false",
									"description": "Prefer Discord app over browser"
								},
								{
									"key": "isMobile",
									"value": "false",
									"description": "Indicate mobile device"
								}
							]
						},
						"description": "Get Discord OAuth2 authorization URL. User should visit this URL to authorize the application."
					}
				},
				{
					"name": "Exchange OAuth Code",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    const jsonData = pm.response.json();",
									"    pm.collectionVariables.set('authToken', jsonData.token);",
									"    pm.environment.set('authToken', jsonData.token);",
									"    console.log('Auth token saved to collection and environment');",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"auth": {
							"type": "noauth"
						},
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"code\": \"OAUTH_AUTHORIZATION_CODE\",\n  \"state\": \"STATE_FROM_PREVIOUS_REQUEST\"\n}"
						},
						"url": {
							"raw": "{{baseUrl}}/auth/oauth/discord/exchange",
							"host": ["{{baseUrl}}"],
							"path": ["auth", "oauth", "discord", "exchange"]
						},
						"description": "Exchange OAuth authorization code for JWT token. After successful authentication, the token is automatically saved to collection variables."
					}
				},
				{
					"name": "Validate Token",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{baseUrl}}/auth/validate",
							"host": ["{{baseUrl}}"],
							"path": ["auth", "validate"]
						},
						"description": "Validate the current JWT token and get user information."
					}
				},
				{
					"name": "Get User Profile",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{baseUrl}}/auth/user/me",
							"host": ["{{baseUrl}}"],
							"path": ["auth", "user", "me"]
						},
						"description": "Get current authenticated user's profile information."
					}
				},
				{
					"name": "Select Guild",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Extract guildId from request body and save it",
									"const requestBody = JSON.parse(pm.request.body.raw);",
									"pm.collectionVariables.set('guildId', requestBody.guildId);",
									"console.log('Guild ID saved to collection variables:', requestBody.guildId);"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"guildId\": \"{{guildId}}\"\n}"
						},
						"url": {
							"raw": "{{baseUrl}}/auth/user/select-guild",
							"host": ["{{baseUrl}}"],
							"path": ["auth", "user", "select-guild"]
						},
						"description": "Select a guild for the current session. Guild ID is saved to collection variables."
					}
				},
				{
					"name": "Logout",
					"request": {
						"method": "POST",
						"header": [],
						"url": {
							"raw": "{{baseUrl}}/auth/logout",
							"host": ["{{baseUrl}}"],
							"path": ["auth", "logout"]
						},
						"description": "Logout and invalidate current session."
					}
				}
			],
			"description": "Authentication and authorization endpoints for OAuth2 flow and user management."
		},
